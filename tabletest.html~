function sizeLeftNav(){
  document.getElementById('leftnav').style.height= (window.innerHeight-100) +"px";
  document.getElementById('mainContent').style.top="25px";
  document.getElementById('mainContent').style.left="205px";
  document.getElementById('mainContent').style.height=(window.innerHeight-100) +"px";
}


